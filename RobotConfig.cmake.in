# - Config file for the ROBOT package
# It defines the following variables
#  ROBOT_INCLUDE_DIRS - include directories for Robot library
#  ROBOT_LIBRARIES    - libraries to link against

# Compute paths
get_filename_component(ROBOT_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(ROBOT_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# These are IMPORTED targets created by RobotConfig.cmake
set(ROBOT_LIBRARIES "@CONF_LIB_DIRS@")
